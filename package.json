{
  "name": "playbooks-transfer",
  "version": "0.0.0",
  "description": "A simple CLI that will fetch a Github repository or subdirectory and transfer it to your account.",
  "homepage": "https://github.com/playbooks-xyz/playbooks-transfer",
  "license": "ISC",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "author": {
    "name": "Playbooks",
    "email": "support@playbooks.xyz",
    "url": "https://github.com/playbooks-xyz"
  },
  "repository": {
    "url": "https://github.com/playbooks-xyz/playbooks-transfer"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "bugs": {
    "url": "https://github.com/playbooks-xyz/playbooks-transfer/issues"
  },
  "keywords": [
    "git",
    "git download",
    "git tarball",
    "git transfer",
    "git unzip",
    "playbooks",
    "playbooks download",
    "playbooks tarball",
    "playbooks transfer",
    "playbooks unzip"
  ],
  "scripts": {
    "start": "vite build --mode development --watch --sourcemap --minify false",
    "build": "vite build",
    "build:typescript": "tsc && vite build && dts-bundle-generator --config ./dts-bundle-generator.config.ts",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier src",
    "format:fix": "prettier --write src",
    "clean": "npm run lint:fix && npm run format:fix",
    "docs:start": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "preversion": "npm run wipe",
    "version": "npm run deploy",
    "predeploy": "npm run build",
    "deploy": "npm publish",
    "postdeploy": "git add . && git push --tags",
    "pregit": "git add .",
    "git": "git push --tags",
    "postgit": "npm run wipe",
    "test": "jest",
    "wipe": "rm -rf ./dist"
  },
  "dependencies": {
    "@octokit/core": "^6.1.2",
    "archiver": "^7.0.1",
    "chalk": "^5.3.0",
    "enquirer": "^2.4.1",
    "fs-extra": "^11.2.0",
    "fstream": "^1.0.12",
    "ora": "5.4.1",
    "sade": "^1.8.1",
    "simple-git": "^3.24.0",
    "unzip-stream": "^0.3.4",
    "vite-plugin-dts": "^3.9.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "@vitejs/plugin-react": "^3.0.0",
    "dts-bundle-generator": "^7.1.0",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "prettier": "^2.8.1",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4",
    "typescript-transform-paths": "^3.4.7",
    "vite": "^4.0.0",
    "vitepress": "^1.0.0-alpha.75"
  }
}
